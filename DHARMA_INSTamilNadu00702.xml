            <div type="edition" xml:lang="tam-Latn" rendition="class:tamil maturity:vernacular">
                
                
                <ab n="a">
                     
                        <lb n="1=1"/>  l.1=1 svasti śrī śakābdam· 1000 5 100 2 10 4 I
                        l.2=2 taṉ mēṟ cellā niṉṟa cupa kiṟi
                        l.3=3 <g type="varuṣam"/> paṅkūṉi <g type="mācam"/> 2 10 2 <g type="tēti"/> miḻalaikkū
                        l.4=4 ṟṟarru naṭuviṟkūṟṟam brahmatēcam 
                        l.5=5 taṉiyūr tirupperuntuṟaiyāṉa pa
                        l.6=6 vittiramāṇikka catuvētimaṅkala
                        l.7=7 ttu nāyaṉār aḷuṭaiya paramacuv(am*)iyaḷ
                        l.8=8 ciṟi paṇṭārattu-k-kaṭavarāṉa paratē-
                        l.9=9 ci muttirai vāmatēva paṇṭāratt=tu
                        l.10=10 kku tirukkāṉai pēṟkūttat=tukku
                        l.11=11 maṇṭali veṇṇaitaṇṭa piḷḷai 
                        l.12=12 [partially covered by cement]
                        l.13=13 namōṅ kāṇivilaippiṟamāṇam pa
                        l.14=14 ṇṇikkuṭuttapaṭi muṉṉāḷ Eṅkaḷ
                        l.15=15 pitiṟukkaḷ taṉamtāṉam peṟṟuṭai
                        l.16=16 caliyaṉ entalukku peruṉāṉ
                        l.17=17 llaiyāvatu kīḻpāṟkellai
                </ab>     
               
                 
            </div>	
            
